<div class="grid-item box"><h1 class="gridText">Tickets</h1><h1 class="gridText">@GetTicketsCount()</h1></div>
@code {
    private int GetTicketsCount()
    {
        Model.DataAccess DA = new Model.DataAccess();
        List<string> result = DA.GetBySubject("SELECT * FROM ticket");
        if (result.Count() > 0)
        {
            return result.Count();
        }
        return 0;

    }
}
