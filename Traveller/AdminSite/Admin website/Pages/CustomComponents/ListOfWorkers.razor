@{
	List<string> staffList = GetListOfStaff();
	for (int i = 0; i < staffList.Count(); i += 2)
	{
		<div class="box WorkerBoxInList">
			<h1>@staffList[i]</h1>
			<h1>@staffList[i + 1]</h1>
		</div>

	}
}
@code {
	private List<string> GetListOfStaff()
	{

		Model.IDataAccess DataAccess = new Model.DataAccess();
		string cmd = "SELECT GetStaff()";
		List<string> result = DataAccess.GetListBySubject(cmd);
		return result;

	}
}
