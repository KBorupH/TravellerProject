
@page
@model Admin_website.Pages.indexModel
@{
	ViewData["Title"] = "Home Page";
}


<Main style="height: 1550px;">
	<div class="wrapper">
		<nav>
			<div class="TopBox" id="TopBoxLeft"></div>
			<h1 class="HeadLiner">Traveller</h1>
			<div class="TopBox" id="TopBoxRight"></div>
		</nav>
		<div class="StatsBoxes">	
			
			<component type="typeof(Pages.CustomComponents.Worker)" render-mode="ServerPrerendered" />

			<div class="grid-item box"><h1 class="gridText">Trains</h1></div>
			<div class="grid-item box"><h1 class="gridText">Tickets</h1></div>
			<div class="grid-item box"><h1 class="gridText">Passengers</h1></div>


		</div>
		<component type="typeof(Pages.CustomComponents.TrainLog)" render-mode="ServerPrerendered" />

		<div id="LeftBox">
			<component type="typeof(Pages.CustomComponents.EarningsGraph)" render-mode="ServerPrerendered" />

			<div class="Train-routes box">
				<div>
					<ol>
					</ol>
				</div>
			</div>
		</div>
	</div>
</Main>